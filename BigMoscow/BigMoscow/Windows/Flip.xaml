<Window x:Class="BigMoscow.Windows.Flip"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        xmlns:properties="clr-namespace:BigMoscow.Properties"
        xmlns:local="clr-namespace:BigMoscow.Windows"
        
        xmlns:bornander="clr-namespace:Bornander.UI.TabCarousel;assembly=Bornander.UI.TabCarousel"  
        xmlns:viewmodels="bi"
        mc:Ignorable="d"
        Title="Flip" Height="1080" Width="1920" WindowStyle = "None" WindowState = "Maximized">
    
    <Window.Background>
        <ImageBrush ImageSource="/UI/MainScreen/mainscreenMain.jpg"/>
    </Window.Background>
    
   
    <Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" />
        
    </Grid>
